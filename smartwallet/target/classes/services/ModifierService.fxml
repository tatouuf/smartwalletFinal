<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.service.ModifierService">

    <GridPane layoutX="50" layoutY="50" hgap="10" vgap="10">

        <TextField fx:id="txtPrix" promptText="Prix" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        <TextField fx:id="txtType" promptText="Type" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        <TextField fx:id="txtStatut" promptText="Statut" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        <TextField fx:id="txtLocalisation" promptText="Localisation" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        <TextField fx:id="txtAdresse" promptText="Adresse" GridPane.rowIndex="4" GridPane.columnIndex="1"/>
        <TextField fx:id="txtTypeService" promptText="TypeService" GridPane.rowIndex="5" GridPane.columnIndex="1"/>

        <Button fx:id="btnEnregistrer" text="Enregistrer" GridPane.rowIndex="6" GridPane.columnIndex="1"
                onAction="#enregistrerModifications"/>
    </GridPane>
</AnchorPane>
